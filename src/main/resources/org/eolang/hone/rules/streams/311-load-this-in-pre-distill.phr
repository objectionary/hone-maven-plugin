---
# yamllint disable rule:line-length
name: load-this-in-distill
pattern: |
  ⟦
    φ ↦ Φ.hone.pre-distill,
    class ↦ 𝑒-class,
    bridge-input ↦ 𝑒-bridge-input,
    bridge-output ↦ 𝑒-bridge-output,
    start ↦ 𝑒-start,
    accepted ↦ 𝑒-accepted,
    returned ↦ 𝑒-returned,
    static ↦ "false",
    body ↦ ⟦ 𝐵-body ⟧
  ⟧
result: |
  ⟦
    φ ↦ Φ.hone.distill,
    class ↦ 𝑒-class,
    bridge-input ↦ 𝑒-bridge-input,
    bridge-output ↦ 𝑒-bridge-output,
    start ↦ 𝑒-start,
    accepted ↦ 𝑒-accepted,
    returned ↦ 𝑒-returned,
    static ↦ "false",
    body ↦ ⟦
      𝜏-load-this ↦ ⟦
        φ ↦ Φ.jeo.opcode.aload,
        i ↦ 0
      ⟧,
      𝐵-body
    ⟧
  ⟧
where:
  - meta: 𝜏-load-this
    function: random-tau
    args: [ 𝐵-body ]
